{
  "resources": {
    "head": [
      {
        "type": "script",
        "src": "./build/app.js"
      }
    ]
  },
  "options": {
    "properties": {
      "container": {
        "order": 1,
        "title": "Place to insert map",
        "type": "object",
        "format": "element"
      },
      "location": {
        "order": 2,
        "title": "Map location",
        "type": "object",
        "properties": {
          "title": {
            "order": 1,
            "title": "Location name",
            "type": "string",
            "default": "Cloudflare Inc."
          },
          "address": {
            "order": 2,
            "title": "Address (Street, City, State and Zip)",
            "type": "string",
            "default": "101 Townsend St, San Francisco, CA 94107"
          },
          "showMarker": {
            "order": 3,
            "title": "Show location marker",
            "type": "boolean",
            "default": true
          }
        }
      },
      "theme": {
        "order": 3,
        "title": "Theme",
        "type": "string",
        "default": "roadmap",
        "enum": [
          "roadmap",
          "satellite",
          "hybrid",
          "simpleGreen",
          "darkGreen",
          "light",
          "midnight",
          "coolBlue",
          "simpleBlackWhite"
        ],
        "enumNames": {
          "roadmap": "Road map",
          "satellite": "Satellite",
          "hybrid": "Satellite with street names",
          "light": "Light",
          "simpleGreen": "Light Green",
          "darkGreen": "Dark Green",
          "coolBlue": "Light Blue",
          "midnight": "Dark Blue",
          "simpleBlackWhite": "Black & White"
        }
      },
      "aspectRatio": {
        "order": 4,
        "title": "Layout",
        "type": "string",
        "enum": [
          "50",
          "100",
          "200"
        ],
        "enumNames": {
          "50": "Horizontal",
          "100": "Square",
          "200": "Vertical"
        },
        "format": "radios",
        "default": "50"
      },
      "zoom": {
        "order": 6,
        "title": "Zoom",
        "type": "number",
        "format": "slider",
        "minimum": 0,
        "maximum": 21,
        "step": 1,
        "default": "13"
      },
